@import "../../wxss/sleeve.wxss";

.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #ffffff;
    padding-top: 40rpx;
}
.title{
    width: 694rpx;
    height: 90rpx;
}
.left{
    display: flex;
    width: 346rpx;
    height: 522rpx;
}
.right-size{
    width: 400rpx;
    height: 260rpx;
    display: flex;
}
.inner-container{
    margin-top: 30rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-bottom: 10rpx;
}
.right-container{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
